{
	"name": "benjisg/hackstack-php",
	"description": "A baseline restful website template to limit reinventing the wheel",
	"keywords": ["web stack","rest","router","twig","hackstack"],
	"homepage": "http://github.com/benjisg/HackStack-PHP",
	"license": "MIT",
	"authors": [
		{
			"name": "Benji Schwartz-Gilbert",
			"email": "benji.schwartzgilbert@gmail.com"
		}
	],
	"require": {
		"php": ">=5.3.0",
		"monolog/monolog": "1.*",
		"slim/slim": "2.*",
		"slim/views": "0.*",
		"twig/twig": "1.*",
		"cartalyst/sentry": "2.0.*",
		"illuminate/database": "4.0.*",
		"ircmaxell/password-compat": "1.0.*",
		"indeyets/pake": "~1.99",
		"swiftmailer/swiftmailer": "@stable"
	},
	"autoload": {
		"psr-4": {
			"Hackstack\\Helpers\\" : "lib/Helpers/"
		}
	},
	"minimum-stability": "stable"
}
